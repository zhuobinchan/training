package com.ehighsun.training.admin.action;

import com.ehighsun.training.base.action.BaseAction;
import com.ehighsun.training.pojo.Student;

public class AdminStudentAction extends BaseAction  {
	private Student student = new Student();
	
	public AdminStudentAction() {
		super();
		setObject(student);
		initialize();
	}
	public Student getStudent() {
		return student;
	}
	public void setStudent(Student student) {
		this.student = student;
	}
	
}
